":59,"column":8},"source":"src\\js\\sign_in.js","original":{"line":1,"column":0}}],"sources":{"src\\js\\sign_in.js":"!function(){\r\n    let view = document.querySelector('.wrapper>.sign_in>form')\r\n    let controller = {\r\n        view: null,\r\n        init: function(view){\r\n            this.view = view\r\n            this.bindEvents()\r\n            // this.ajaxModule()\r\n        },\r\n        bindEvents: function(){\r\n            this.view.addEventListener('submit', (e)=>{\r\n                e.preventDefault()\r\n                let needText = {'email':'邮箱','password':'密码','confirmPassword':'确认密码'}\r\n                let need = ['email','password','confirmPassword']\r\n                let hash = {}\r\n                this.clearData()\r\n                this.gatherData(need,hash)\r\n                this.verifyData(need,needText,hash)\r\n            }, false)\r\n        },\r\n        clearData: function(){\r\n            this.view.querySelectorAll('span[class=error]').forEach(function(value){\r\n                value.innerHTML = ''\r\n            })\r\n        },\r\n        gatherData: function(need,hash){\r\n            need.forEach((value)=>{\r\n                hash[value] = this.view.querySelector(`[name=${value}]`).value\r\n            })\r\n            return hash\r\n        },\r\n        verifyData: function(need,needText,hash){\r\n            if(hash['email'].indexOf('@') === -1){\r\n                this.view.querySelector(`[name=email]`).nextElementSibling.innerHTML = `邮箱格式不正确`\r\n            }\r\n            if(hash['password'] !== hash['confirmPassword']){\r\n                this.view.querySelector('[name=confirmPassword]').nextElementSibling.innerHTML = '确认密码错误'\r\n            }\r\n            need.forEach((value)=>{\r\n                if(hash[value] === ''){\r\n                    this.view.querySelector(`[name=${value}]`).nextElementSibling.innerHTML = `请输入${needText[value]}`\r\n                }\r\n            })\r\n        }\r\n        // ajaxModule: function(){\r\n        //     var XHR = new XMLHttpRequest()\r\n        //     XHR.onreadystatechange = function(){}\r\n        //     XHR.open('POST','/sign_in')\r\n        //     XHR.send()\r\n        // }\r\n    }\r\n    controller.init.call(controller,view)\r\n}.call()"},"lineCount":null}},"hash":"bc92ab3f6e3cd20f75e2dc718ab9321b","cacheData":{"env":{}}}